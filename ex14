-- Default.aspx

Partial Class _Default
Inherits System.Web.UI.Page
Protected Sub Button1_Click(sender As Object, e As EventArgs) Handles Button1.Click
If IsValid Then
MsgBox("Account Created Successfully")
End If
End Sub
End Class

-- Web.config

<configuration>
<appSettings>
<add key="ValidationSettings:UnobstructiveValidationMode" value="None"/>
</appSettings>
</configuration>
